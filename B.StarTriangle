public class StarTriangle
{
    int width;

    public StarTriangle(int wdth)
    {
        width=wdth;
    }

    public String toString()
    {
        for (int i=0; i<width; i++)
        {
            for (int j=0; j<=i; j++)
            {
                System.out.print("[*]");
            }
            System.out.println();
        }
        return "";
    }
}
