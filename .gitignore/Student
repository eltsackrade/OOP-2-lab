public class Student {
    private String studName;
    private int studId;
    static int year;

    public Student (String name, int id)
    {
        studName=name;
        studId=id;
    }

    public setData (String name, int id)
    {
        studName=name;
        studId=id;
    }

    public String getStudentName ()
    {
        return studName;
    }

   public int getStudId()
    {
        return studId;
    }
}
